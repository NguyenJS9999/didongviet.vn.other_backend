{
  "name": "project-struct",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "make:model": "npx sequelize model:generate --name %npm_config_name% --attributes id:integer",
    "make:migration": "npx sequelize migration:generate --name %npm_config_name%",
    "migrate": "npx sequelize db:migrate",
    "migrate:undo": "npx sequelize db:migrate:undo",
    "migrate:reset": "npx sequelize db:migrate:undo:all",
    "migrate:refresh": "npm run migrate:reset && npm run migrate",
    "make:seeder": "npx sequelize-cli seed:generate --name %npm_config_name%",
    "db:seed": "npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-session": "^1.17.3",
    "http-errors": "~1.6.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "~1.9.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.8",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.13",
    "sequelize": "^6.37.1",
    "uuid": "^9.0.1",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.2"
  }
}
